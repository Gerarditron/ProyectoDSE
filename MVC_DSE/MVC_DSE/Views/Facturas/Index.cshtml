@model IEnumerable<MVC_DSE.Models.Factura>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Concepto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFactura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdBitacoraNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoProcesoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Concepto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFactura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdBitacoraNavigation.IdBitacora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoProcesoNavigation.IdTipoProceso)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.IdFactura">Edit</a> |
                <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.IdFactura">Details</a> |
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.IdFactura">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
